{
  "expo": {
    "name": "Proffy",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8257E5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rian.proffy",
      "buildNumber": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "backgroundColor": "#8257E5",
      "build": {
        "babel": {
          "include": ["static-container"]
        }
      }
    },
    "android": {
      "package": "com.rian.proffy",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#8257E5"
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#8257E5"
    }
  }
}
